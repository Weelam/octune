-- Darude Sandstorm

main = { 136 : [+ melody bass +] }

melody = [* 2 :
    [* 5 : detB4 *] <0.25_>
    [* 7 : detB4 *] <0.25_>
    [* 7 : detE5 *] <0.25_>
    [* 7 : detD5 *] <0.25_>
    [* 2 : detA4 *]
    [* 5 : detB4 *] <0.25_>
    [* 7 : detB4 *] <0.25_>
    [* 2 : detE5 *]
    [* 5 : detB4 *] <0.25_>
    [* 7 : detB4 *] <0.25_>
    [* 2 : detE5 *]
*]

detA4 = [ [+ <0.2A4> <0.2D4> +] <0.05_> ]
detB4 = [ [+ <0.2B4> <0.2E4> +] <0.05_> ]
detD5 = [ [+ <0.2D5> <0.2F#4> +] <0.05_> ]
detE5 = [ [+ <0.2E5> <0.2C5> +] <0.05_> ]

bass = [* 2 :
    [* 7 : bassE *]
    [* 4 : bassC *]
    [* 4 : bassD *]
    bassDSharp
    [* 7 : bassE *]
    bassA
    [* 7 : bassE *]
    bassD
*]

bassC = [ [+ <0.25C3> <0.25C2> +] <0.25_>]
bassD = [ [+ <0.25D3> <0.25D2> +] <0.25_>]
bassDSharp = [ [+ <0.25D#3> <0.25D#2> +] <0.25_>]
bassE = [ [+ <0.25E3> <0.25E2> +] <0.25_>]
bassA = [ [+ <0.25A3> <0.25A2> +] <0.25_>]